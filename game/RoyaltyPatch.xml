<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Royalty</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>

    <li Class="PatchOperationAdd">
      <xpath>/Defs/FactionDef[defName="Empire"]/permanentEnemyToEveryoneExcept</xpath>
      <value>
        <li>TSF_PlayerRonin</li>
      </value>
    </li>

    <li Class="PatchOperationReplace">
      <xpath>/Defs/ThingStyleDef[defName="PrestigeMarineHelmet_Samurai"]/overrideLabel</xpath>
      <value>
        <overrideLabel>prestige samurai marine helmet</overrideLabel>
      </value>
    </li>

    <li Class="PatchOperationAdd">
      <xpath>/Defs/ThingStyleDef[defName="PrestigeMarineHelmet_Samurai"]/graphicData</xpath>
      <value>
        <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
      </value>
    </li>

    <li Class="PatchOperationAdd">
      <xpath>/Defs/ThingDef[defName="TSF_OniKanabo"]</xpath>
      <value>
        <soundInteract>Interact_ZeusHammer</soundInteract>
        <meleeHitSound>MeleeHit_ZeusHammer</meleeHitSound>
      </value>
    </li>

  </operations>
  </match>
  </Operation>
</Patch>
